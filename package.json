{
  "name": "@massalabs/as",
  "version": "1.1.0",
  "description": "Massalabs Native Assemblyscript Implementations",
  "main": "assembly/index.ts",
  "types": "assembly/index.ts",
  "type": "module",
  "author": "Massa Labs <info@massa.net>",
  "contributors": [
    "Evgeni Pirianov evgeni.pirianov@gmail.com",
    "Gr√©gory Libert <greg_libert@yahoo.fr>"
  ],
  "license": "(MIT AND Apache-2.0)",
  "homepage": "https://github.com/massalabs/as",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massalabs/as"
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "clean": "rimraf \"./transformer/dist\"",
    "install:transformer": "npm install --prefix ./transformer",
    "build:transformer": "tsc -p transformer",
    "build": "npm run clean && npm run build:transformer",
    "test": "asp",
    "test:ci": "asp --summary",
    "prettier": "as-prettier \"**/*.ts\" --check",
    "prettier:fix": "as-prettier \"**/*.ts\" --write",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "fmt:check": "npm run prettier && npm run lint",
    "fmt": "npm run prettier:fix && npm run lint:fix",
    "prepublish": "npm run install:transformer",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@as-pect/cli": "^8.0.0",
    "@assemblyscript/loader": "^0.24.1",
    "@massalabs/as-transformer": "file:./transformer",
    "@massalabs/prettier-config-as": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "assemblyscript": "^0.24.1",
    "assemblyscript-prettier": "^1.0.2",
    "eslint": "^8.12.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3"
  },
  "prettier": "@massalabs/prettier-config-as"
}
